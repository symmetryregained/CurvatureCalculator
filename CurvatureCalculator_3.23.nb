Cell[BoxData[
 RowBox[{
  RowBox[{"covD", "[", 
   RowBox[{"obj_", ",", "connect_", ",", "coords_", ",", "invtet_"}], "]"}], ":=", 
  RowBox[{"Simplify", "[", 
   RowBox[{"Table", "[", 
