Cell["\<\
The Initialize function (coords, metric, tetrad and tetrad derivatives)\
\>", "Subsubsection",
 CellChangeTimes->{{3.688987081350943*^9, 3.6889870937516527`*^9}, {
  3.704624608541992*^9, 3.7046246340146484`*^9}},
 ExpressionUUID -> "f666c64b-23f8-4a5d-9434-398a7e51285f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"InitializeModel", "[", "geom_", "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "=", 
     RowBox[{"Length", "[", 
      RowBox[{"geom", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
      "]"}]}], ";", "  ", 
    RowBox[{"(*", " ", 
     RowBox[{"defines", " ", "the", " ", "dimension"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"xx", "=", 
     RowBox[{"geom", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}],
     ";", "\[IndentingNewLine]", 
    RowBox[{"dxx", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"ToExpression", "[", 
        RowBox[{"\"\<d\>\"", "<>", 
         RowBox[{"ToString", "[", "#", "]"}]}], "]"}], "&"}], "/@", "xx"}]}], 
    ";", "  ", 
    RowBox[{"(*", " ", 
     RowBox[{"writes", " ", "differentials"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"tetrad", "=", 
     RowBox[{"geom", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}],
     ";", "\[IndentingNewLine]", 
    RowBox[{"g", "=", 
     RowBox[{"geom", "\[LeftDoubleBracket]", 
      RowBox[{"3", ",", "1"}], "\[RightDoubleBracket]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"sgn", "=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"geom", "\[LeftDoubleBracket]", 
           RowBox[{"3", ",", "2"}], "\[RightDoubleBracket]"}], "==", "1"}], 
         ")"}], "||", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"geom", "\[LeftDoubleBracket]", 
           RowBox[{"3", ",", "2"}], "\[RightDoubleBracket]"}], "==", 
          RowBox[{"-", "1"}]}], ")"}]}], ",", 
       RowBox[{"geom", "\[LeftDoubleBracket]", 
        RowBox[{"3", ",", "2"}], "\[RightDoubleBracket]"}], ",", 
       RowBox[{
       "Throw", "[", "\"\<Wrong signature: has to be 1 or -1 !\>\"", "]"}]}], 
      "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Id", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"i", "\[NotEqual]", "j"}], ",", "0", ",", "1"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"tetin", "=", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"PowerExpand", "[", 
       RowBox[{"Inverse", "[", "tetrad", "]"}], "]"}], "]"}]}], ";", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{"inverse", " ", "of", " ", "the", " ", "tetrad"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"gin", "=", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"PowerExpand", "[", 
       RowBox[{"Inverse", "[", "g", "]"}], "]"}], "]"}]}], ";", "     ", 
    RowBox[{"(*", " ", 
     RowBox[{"inverse", " ", "of", " ", "the", " ", "metric"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"A", "=", 
     RowBox[{"PowerExpand", "[", 
      RowBox[{"Simplify", "[", 
       SuperscriptBox[
        RowBox[{"Sqrt", "[", 
         RowBox[{"(", 
          RowBox[{"sgn", " ", 
           RowBox[{"Det", "[", "g", "]"}]}], ")"}], "]"}], 
        FractionBox["1", "n"]], "]"}], "]"}]}], ";", "   ", 
    RowBox[{"(*", " ", 
     RowBox[{"the", " ", "scale", " ", "density"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"gbar", "=", 
     RowBox[{"Cancel", "[", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        SuperscriptBox["A", 
         RowBox[{"-", "2"}]], "g"}], "]"}], "]"}]}], ";", "   ", 
    RowBox[{"(*", " ", 
     RowBox[{"the", " ", "shape", " ", "density"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"gbarin", "=", 
     RowBox[{"Cancel", "[", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"Inverse", "[", "gbar", "]"}], "]"}], "]"}]}], ";", " ", 
    RowBox[{"(*", " ", 
     RowBox[{"inverse", " ", "of", " ", "the", " ", "metric"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Define", " ", "non"}], "-", 
       RowBox[{"holonomic", " ", "basis", " ", "vectors", " ", 
        RowBox[{"(", 
         RowBox[{"non", "-", 
          RowBox[{"holonomic", " ", "partial", " ", "derivatives"}]}], 
         ")"}]}]}], ",", 
      RowBox[{
      "and", " ", "antisymmetrized", " ", "derivatives", " ", "and", " ", 
       "products", " ", 
       RowBox[{"(", 
        RowBox[{
        "to", " ", "be", " ", "used", " ", "in", " ", "definition", " ", "of",
          " ", "the", " ", "Riemann", " ", "tensor"}], ")"}]}]}], " ", "*)"}],
     "\n", 
    RowBox[{
     RowBox[{"del", "[", 
      RowBox[{"obj_", ",", "coords_", ",", "invtet_"}], "]"}], ":=", 
     RowBox[{"Simplify", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"invtet", "\[LeftDoubleBracket]", 
            RowBox[{"mu", ",", "ind"}], "\[RightDoubleBracket]"}], 
           RowBox[{"D", "[", 
            RowBox[{"obj", ",", 
             RowBox[{
             "coords", "\[LeftDoubleBracket]", "mu", 
              "\[RightDoubleBracket]"}]}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"mu", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"ind", ",", "1", ",", 
          RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}], "]"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"AdT", "[", 
      RowBox[{"obj_", ",", "coords_", ",", "invtet_"}], "]"}], ":=", 
     RowBox[{"Simplify", "[", 
      RowBox[{"2", 
       RowBox[{"Symmetrize", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", 
          RowBox[{
           RowBox[{"Simplify", "[", 
            RowBox[{"del", "[", 
             RowBox[{"obj", ",", "coords", ",", "invtet"}], "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"3", ",", "1", ",", "2", ",", "4"}], "}"}]}], "]"}], ",", 
         RowBox[{"Antisymmetric", "[", 
          RowBox[{"{", 
           RowBox[{"3", ",", "4"}], "}"}], "]"}]}], "]"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"AT2", "[", 
      RowBox[{"objudd1_", ",", "objudd2_"}], "]"}], ":=", 
     RowBox[{"Simplify", "[", 
      RowBox[{"2", 
       RowBox[{"Symmetrize", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", 
          RowBox[{
           RowBox[{"Simplify", "[", 
            RowBox[{
             RowBox[{"Transpose", "[", 
              RowBox[{"objudd1", ",", 
               RowBox[{"{", 
                RowBox[{"1", ",", "3", ",", "2"}], "}"}]}], "]"}], ".", 
             "objudd2"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "3", ",", "2", ",", "4"}], "}"}]}], "]"}], ",", 
         RowBox[{"Antisymmetric", "[", 
          RowBox[{"{", 
           RowBox[{"3", ",", "4"}], "}"}], "]"}]}], "]"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"coframe", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"DisplayForm", "[", 
        RowBox[{"RowBox", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Superscript", "[", 
            RowBox[{"\[Theta]", ",", 
             RowBox[{"ToString", "[", 
              RowBox[{
              "xx", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
              "]"}]}], "]"}], ",", "\"\< = \>\"", ",", 
           RowBox[{"TraditionalForm", "[", 
            RowBox[{
             RowBox[{
             "tetrad", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}],
              ".", "dxx"}], "]"}]}], "}"}], "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"ds", "=", 
     RowBox[{"Collect", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"dxx", ".", 
          RowBox[{"Transpose", "[", "tetrad", "]"}], ".", "g", ".", "tetrad", 
          ".", "dxx"}], "//", "PowerExpand"}], "//", "Cancel"}], ",", "dxx", 
       ",", "Simplify"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Column", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
       "\"\<\!\(\*SubscriptBox[SuperscriptBox[\(E\), \(A\)], \(\[Mu]\)]\) = \
\>\"", " ", 
        RowBox[{"MatrixForm", "[", 
         RowBox[{"TraditionalForm", "[", "tetrad", "]"}], "]"}]}], ",", " ", 
       RowBox[{"\"\<\!\(\*SubscriptBox[\(g\), \(AB\)]\) = \>\"", " ", 
        RowBox[{"MatrixForm", "[", 
         RowBox[{"TraditionalForm", "[", "g", "]"}], "]"}]}], ",", 
       RowBox[{
       "\"\<\!\(\*SuperscriptBox[\(\[Theta]\), \(A\)]\) = \
\!\(\*SubscriptBox[SuperscriptBox[\(E\), \(A\)], \
\(\[Mu]\)]\)\!\(\*SuperscriptBox[\(dx\), \(\[Mu]\)]\) => \>\"", 
        RowBox[{"MatrixForm", "[", "coframe", "]"}]}], ",", " ", 
       RowBox[{
       "\"\<\!\(\*SuperscriptBox[\(ds\), \(2\)]\) = \!\(\*SubscriptBox[\(g\), \
\(\[Mu]\[Nu]\)]\)\!\(\*SuperscriptBox[\(dx\), \
\(\[Mu]\)]\)\!\(\*SuperscriptBox[\(dx\), \(\[Nu]\)]\) = \
\!\(\*SubscriptBox[\(g\), \(AB\)]\)\!\(\*SuperscriptBox[\(\[Theta]\), \
\(A\)]\)\!\(\*SuperscriptBox[\(\[Theta]\), \(B\)]\) = \>\"", " ", 
        RowBox[{"TraditionalForm", "[", "ds", "]"}]}]}], "}"}], "]"}]}], 
   ")"}]}]], "Input",
